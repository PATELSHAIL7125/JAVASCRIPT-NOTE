// basically jo string pehla hase to ane string consider karvama avse 
//and pela numbers hase ane string pachi hase to pela numbers ne operators apply thase pachi string joint thase
 
console.log("1"+2);//ans is 12
console.log(1+"2");//ans is 12
console.log("1"+"2");//ans is 12
console.log("1"+2+2);//ans is 122
console.log(1+2+"2");//ans is 32

let gamecounter=100;
gamecounter++;
console.log(gamecounter)// ans is 101
let num;
let valuecounter=100;
num=valuecounter--;//if prefix operator than ans is 99 and 99
console.log(valuecounter)// ans is 99
console.log(num)//ans is 100

/********* comparision operator *************/

//this type of example is not suggest
console.log(null>0)// null converted into a number as treated 0 than this is false
console.log(null==0) //null converted into a number as treated 0 than this is false
console.log(null>=0)// null converted into a number as treated 0 than this is true(for graeter or equal to)

//=== convertion

console.log("2"===2)//ans is false because in === convertion check data type in this example string and number  is compared than ans is false

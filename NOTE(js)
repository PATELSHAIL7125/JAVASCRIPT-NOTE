console.log(typeof null)// null ni data type object che

let score="33abc"


console.log(typeof score);
console.log(typeof (score));

let valueinnumber= Number(score)// Number(score) means score ne e number ma convert karse
console.log(typeof valueinnumber);// valueinnumber e score ne number data type ma convert kare che
console.log(valueinnumber);// for 33abc mate ane print not a number karse

let value=null;


console.log(typeof value);
console.log(typeof (value));

let typeinnumber= Number(score)// Number(score) means score ne e number ma convert karse
console.log(typeof typeinnumber);// valueinnumber e score ne number data type ma convert kare che
console.log(typeinnumber)//null mate 0 print thase
